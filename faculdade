programa
{
	
	funcao inicio()
	{
		cadeia nome 
		cadeia faculdade
		cadeia curso
		escreva ("qual seu nome? : ")
		leia (nome)
		escreva ( "qual sua faculdade? :" )
		leia (faculdade)
		escreva (" qual curso seu curso? :")
		leia (curso)
		escreva ("ola "+nome+" esse é seu curso de     "+curso+" na instituição "+faculdade+ " .")
		
		
	}
}
